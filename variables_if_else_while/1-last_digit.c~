	/* your code goes there */

	int ("ld = n%10");
	
	if (n > 5)
	  {
	    printf("Last digit of %d is %d/n"), n, ld;
	      }
	
	else if
	  {
	    printf("Last digit of", "n", "is", "%d/n"), n
	      }

	else
	  {
	    printf("Last digit of", "n", "is", "%d/n"), n
	      }

	
